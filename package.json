{
  "name": "prisma-apollo-api",
  "description": "A prisma api starter kit.",
  "version": "1.0.0",
  "main": "main.js",
  "author": "FacinouFabian <fabianfacinou02@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/FacinouFabian/create-prisma-api",
  "bugs": "https://github.com/FacinouFabian/create-prisma-api/issues",
  "repository": {
    "url": "https://github.com/FacinouFabian/create-prisma-api",
    "type": "git"
  },
  "keywords": [
    "prisma",
    "api",
    "starter-kit",
    "typescript"
  ],
  "files": [
    "main.js",
    "dist/*.js",
    "bin/*.js"
  ],
  "bin": {
    "create-prisma-api": "bin/create-prisma-api.js"
  },
  "scripts": {
    "dev": "yarn nodemon",
    "start": "yarn build && node .dist/main.js",
    "clean": "yarn rimraf .dist",
    "build": "yarn clean && yarn babel src/ --out-dir .dist/ --extensions \".ts\" --copy-files",
    "test": "jest --config jestconfig.json",
    "format": "yarn prettier --write ./src/**/*.ts",
    "format:prisma": "npx prisma format",
    "build:db": "prisma migrate dev --name init",
    "prepublishOnly": "yarn test",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/chalk": "^2.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^9.0.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "prisma": "^5.11.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.2.0",
    "@prisma/client": "^3.0.2",
    "apollo-server": "^3.3.0",
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13"
  }
}
